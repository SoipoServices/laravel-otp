{
    "name": "soiposervices/laravel-otp",
    "description": "Laravel OTP generator and validation",
    "homepage": "https://github.com/SoipoServices/laravel-otp",
    "keywords": [
        "laravel",
        "otp",
        "one time passwords",
        "one time pin",
        "email validation",
        "mobile validation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "SoipoServices",
            "email": "info@soiposervices.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "SoipoServices\\Otp\\OtpServiceProvider"
            ],
            "aliases": {
                "Otp": "SoipoServices\\Otp\\OtpFacade"
            }
        }
    },
    "require": {
        "php": "^5.5.9|^7.0|^8.0|^8.1|^8.2",
        "illuminate/support": "^5.2|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "SoipoServices\\Otp\\": "src"
        }
    }
}
